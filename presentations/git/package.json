  "dependencies": {
    "@layouts/slidev-extra": "workspace:*",
    "@slidev/cli": "^52.2.4",
    "@slidev/theme-default": "latest",
    "katex": "^0.16.22",
    "vue": "^3.5.21"
  }
}
